{
  "name": "portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "tsc": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "organise-imports": "organize-imports-cli tsconfig.json",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "gen": "npm run gen:configuration-service && npm run gen:fix:headers && npm run organise-imports && npm run format:write",
    "gen:fix:headers": "find src/services -type f -exec sed -i '' 's/headers\\[/headers?.[/g' {} \\;",
    "gen:configuration-service": "rimraf src/services/configuration-service && swagger2openapi http://localhost:8080/swagger/doc.json > openapi.json && openapi-generator generate -i openapi.json -g typescript-axios -o ./src/services/configuration-service"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "i18next": "^23.7.6",
    "mobx": "^6.11.0",
    "mobx-react-lite": "^4.0.5",
    "mobx-state-tree": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gauge-chart-es": "0.4.1-4",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.18.0",
    "recharts": "^2.9.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-gauge-chart": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.0.9",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "organize-imports-cli": "^0.10.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prop-types": "^15.8.1",
    "rimraf": "^5.0.5",
    "swagger2openapi": "^7.0.8",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "resolutions": {
    "react": "^18.2.0",
    "@types/react": "^18.2.37"
  }
}
